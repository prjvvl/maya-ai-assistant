		"@types/node": "^22.15.21",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.28.1",
		"svelte-check": "^4.1.6",
		"typescript": "~5.8.3",
		"vite": "^6.3.5"
	},
	"dependencies": {
		"@google/generative-ai": "^0.24.1",
		"@sveltejs/kit": "^2.21.1"
