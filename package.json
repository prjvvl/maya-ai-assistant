	"type": "module",
	"keywords": [
		"ai",
		"voice",
		"assistant",
		"svelte",
		"gemini",
		"speech",
		"sveltekit",
		"typescript"
	],
	"author": "Prajwal",
	"repository": {
		"type": "git",
		"url": "https://github.com/prajwal/maya.git"
	},
	"homepage": "https://github.com/prajwal/maya#readme",
	"bugs": {
		"url": "https://github.com/prajwal/maya/issues"
	},
	"engines": {
		"node": ">=18.0.0 <22.0.0"
	},
	"scripts": {
		"dev": "vite dev",
